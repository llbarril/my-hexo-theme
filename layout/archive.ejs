<%
    var title = '';
    if (page.category) page.title = page.category;
    if (page.tag) page.title = page.tag;
    if (page.archive){ 
  		
      page.title = theme.archivesTitle.name;   
    }
%>
<div class="row"> 
	<div class="col-md-8">
		<% page.posts.each(function(item){ %>
			<%- partial('_partial/article-archive', {item: item}) %>
		<% }); %>

		<%- partial('_partial/pagination') %>
	</div>

	<%- partial('_partial/sidebar') %>
</div>